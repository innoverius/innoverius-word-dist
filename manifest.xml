<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>516f493b-d7df-4a1f-b018-02d556218df5</Id>
  <Version>17.0.0.1</Version>
  <ProviderName>Innoverius</ProviderName>
  <DefaultLocale>nl-BE</DefaultLocale>
  <DisplayName DefaultValue="Innoverius"/>
  <Description DefaultValue="Add-in to use document automation and artificial intelligence in cases."/>
  <IconUrl DefaultValue="https://innoverius.github.io/word-public/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://innoverius.github.io/word-public/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://www.innoverius.com/support"/>
  <AppDomains>
    <AppDomain>https://www.innoverius.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://innoverius.github.io/word-public/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Control xsi:type="Button" id="TaskpaneButton">
                <Label resid="TaskpaneButton.Label"/>
                <Supertip>
                  <Title resid="TaskpaneButton.Label"/>
                  <Description resid="TaskpaneButton.Tooltip"/>
                </Supertip>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Action xsi:type="ShowTaskpane">
                  <TaskpaneId>ButtonId1</TaskpaneId>
                  <SourceLocation resid="Taskpane.Url"/>
                </Action>
              </Control>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://innoverius.github.io/word-public/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://innoverius.github.io/word-public/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://innoverius.github.io/word-public/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://innoverius.github.io/word-public/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://innoverius.github.io/word-public/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Innoverius"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to open the Innoverius task pane"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>